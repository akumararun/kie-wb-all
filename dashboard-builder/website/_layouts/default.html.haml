!!!
%html(lang="en" itemscope="" itemtype="http://schema.org/Product" xmlns="http://www.w3.org/1999/xhtml")
  %head
    %meta(http-equiv="Content-Type" content="text/html; charset=UTF-8")
    %meta(name="viewport" content="width=device-width, initial-scale=1.0")
    %title #{escape_once(site.title + " - " + page.title)}
    %meta(name="description"){:content => page.description ? page.description : site.description}
    %meta(name="keywords"){:content => page.keywords ? page.keywords : site.keywords}
    %link{:href => "../css/bootstrap.min.css", :media => "screen", :rel => "stylesheet"}/
    %link{:href => "../css/bootstrap-community.min.css", :media => "screen", :rel => "stylesheet"}/
    %link{:href => "../css/index-pages.css", :media => "screen", :rel => "stylesheet"}/
    /[if lt IE 9]
      <script src="http://static.jboss.org/theme/js/libs/html5/pre3.6/html5.min.js"></script>
    %link{:href => "http://static.jboss.org/example/images/favicon.ico", :rel => "shortcut icon"}/
    %link{:href => "http://static.jboss.org/example/images/apple-touch-icon-144x144-precomposed.png", :rel => "apple-touch-icon-precomposed", :sizes => "144x144"}/
    %link{:href => "http://static.jboss.org/example/images/apple-touch-icon-114x114-precomposed.png", :rel => "apple-touch-icon-precomposed", :sizes => "114x114"}/
    %link{:href => "http://static.jboss.org/example/images/apple-touch-icon-72x72-precomposed.png", :rel => "apple-touch-icon-precomposed", :sizes => "72x72"}/
    %link{:href => "http://static.jboss.org/example/images/apple-touch-icon-precomposed.png", :rel => "apple-touch-icon-precomposed"}/
    %link{:rel => "shortcut icon", :href => relative("/favicon.ico")}
    :css
      @media (min-width: 980px) {
      .banner {
      	background-image: url(../images/banner-1180px.png);
      	height: 110px;
      }
      }
       @media (max-width: 979px) {
      .banner {
      	background-image: url(../images/logo.png);
      	background-repeat: no-repeat;
      	height: 60px;
      }
      }
       @media (max-width: 650px) {
      .banner {
      	width: 200px;
      	margin: 0px auto;
      }
      }
    %script{:async => "", :src => "../js/elqCfg.min.js", :type => "text/javascript"}
    %script{:src => "../js/jquery-1.9.1.min.js"}
    %script{:src => "../js/opt_content.js"}
    %script{:src => "../js/base_single_icon.js", :type => "text/JavaScript"}
    %script{:src => "../js/dojo.xd.js", :type => "text/JavaScript"}
    %script{:src => "../js/get.jsp", :type => "text/JavaScript"}
  %body
    .forkMeOnGithub
      %a{:href => "https://github.com/droolsjbpm/dashboard-builder"}
        %img{:alt => "Fork me on GitHub", :src => "../images/forkMeOnGithub.png"}/
    #tabnav-panel.tabnav-closed
      #tabnav.tabnavclearfix
        .tabcontent
          %p.overview
            Like
            = succeed "?" do
              %span#project_name Example
            It’s part of a community of Red Hat
            projects. Learn more about Red Hat and our open source communities:
          .row-fluid
            %span.span4.middlewarelogo
              %img{:alt => "Red Hat JBoss MIDDLEWARE", :src => "../images/RHJB_Middleware_Logotype.png"}/
            %span.span4
            %ul.level1
              %li.leaf
                %a{:href => "http://example.jboss.org/index.html#"} Red Hat JBoss Middleware Overview
              %li.leaf
                %a{:href => "http://example.jboss.org/index.html#"} Red Hat JBoss Middleware Products
              %li.leaf
                %a{:href => "http://example.jboss.org/index.html#"} Red Hat JBoss Projects &amp; Standards
            %span.span4
            %ul.level1
              %li.leaf
                %a{:href => "http://example.jboss.org/index.html#"} redhat.com
              %li.leaf
                %a{:href => "http://example.jboss.org/index.html#"} Red Hat Customer Portal
              %li.leaf
                %a{:href => "http://example.jboss.org/index.html#"} OpenShift
    %ul#top.visuallyhidden
      %li
        %a{:accesskey => "n", :href => "http://example.jboss.org/index.html#nav", :title => "Skip to navigation"}
          Skip to
          navigation
      %li
        %a{:accesskey => "c", :href => "http://example.jboss.org/index.html#page", :title => "Skip to content"} Skip to content
    #content.container
      <div class=" socialIcons"">
      = succeed "&nbsp;" do
        %a{:href => "https://twitter.com/@dashbuilder", :target => "_blank"}
          %img{:src => "../images/twiter_icon.png", :title => "@dashbuilder"}/
      %a{:href => "#"}
        %img{:src => "../images/google+_icon.png", :title => "Google+"}/
    .dropup
      %a#tab.tabnav-closed{:href => "https://www.jboss.org/"} Red Hat
    .banner
      %a{:href => "http://example.jboss.org/index.html#"}
      .taglinelight.visible-desktop Open source dashboards and reporting platform
    #navbar-fix.navbar.navbar-inverse
      .navbar-inner
        .container
          %a.btn.btn-navbar{"data-target" => ".nav-collapse", "data-toggle" => "collapse"}
            %span.icon-bar
            %span.icon-bar
            %span.icon-bar
          .nav-collapse.collapse
            %ul.nav
              %li
                %a{:href => "index.html"} Home
              %li
                %a{:href => "about.html"} About
              %li.dropdown
                %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"}
                  Downloads
                  %b.caret
                %ul.dropdown-menu
                  %li
                    %a{:href => "downloads_binaries.html"} Binaries Download
                  %li
                    %a{:href => "downloads_quick_start.html"} Quick Start Demo
              %li.dropdown
                %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"}
                  Learn
                  %b.caret
                %ul.dropdown-menu
                  %li
                    %a{:href => "documentation.html"} Documentation
                  / <li><a href="videos.html">Video's</a></li>
                  %li
                    %a{:href => "slides.html"} Slides
                  / <li><a href="testimonials.html">Testimonials and Case Studies</a></li>
              %li.dropdown
                %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"}
                  Get Help
                  %b.caret
                %ul.dropdown-menu
                  %li
                    %a{:href => "forum.html"} Forum (free support)
                  %li
                    %a{:href => "chat.html"} Chat real-time
                  %li
                    %a{:href => "report_issue.html"} Report an issue
                  %li
                    %a{:href => "paid_support.html"} Commercial support
              %li.dropdown
                %a.dropdown-toggle{"data-toggle" => "dropdown", :href => "#"}
                  Join Us
                  %b.caret
                %ul.dropdown-menu
                  %li
                    %a{:href => "source_code.html"} Source code
                  %li
                    %a{:href => "continous_integration.html"} Continous integration
                  %li
                    %a{:href => "team.html"} Team
                  %li
                    %a{:href => "license.html"} License
                  %li
                    %a{:href => "research.html"} Research
              %li.divider
            %ul.nav.pull-right
              %li
                / <form class="navbar-search"> <input class="search-query" id="projectsearch" placeholder="Search" type="text"></form>
    ~ content
    %footer.navbar-inner{:style => "font-size: 80%;"}
      .row-fluid.span12
        .span6{:style => "color: #FFF !important; float: left;"}
          Development is sponsored by
          %a{:href => "http://www.redhat.com/"}
            %img{:alt => "Red Hat", :src => "../images/redhat_logo.png"}/
        .span6{:style => "white-space:nowrap; float:right; color: #FFF !important;"}
          © Copyright 2006-2013,
          Red Hat, Inc. or third-party contributors
          %a{:href => "http://www.redhat.com/legal/legal_statement.html"}
            Terms
            of use
          \-
          %a{:href => "http://www.redhat.com/legal/privacy_statement.html"} Privacy policy
          \-
          %a{:href => "website_info.html"} Website info
      %center{:style => "color: #FFF !important;"}
        Dashbuilder is part of the
        %br/
        %a{:href => "http://www.jboss.org/", :target => "_blank"}
          %img{:src => "../images/jbossbadge.png", :style => "background:#FFF;"}/
    %span.backToTop{:style => "display: none;"}
      %a{:href => "http://example.jboss.org/index.html#top"}
        back to
        top
