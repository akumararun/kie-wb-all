  <fixtures>
    <fact-data>
      <type>Driver</type>
      <name>d1</name>
      <fieldData>
        <field-data>
          <name>age</name>
          <value>42</value>
          <nature>0</nature>
        </field-data>
        <field-data>
          <name>name</name>
          <value>david</value>
          <nature>0</nature>
        </field-data>
      </fieldData>
      <isModify>false</isModify>
    </fact-data>
    <fact-data>
      <type>Driver</type>
      <name>d2</name>
      <fieldData>
        <field-data>
          <name>name</name>
          <value>michael</value>
          <nature>0</nature>
        </field-data>
      </fieldData>
      <isModify>false</isModify>
    </fact-data>
    <execution-trace/>
    <expect-fact>
      <fieldValues>
        <expect-field>
          <fieldName>age</fieldName>
          <expected>42</expected>
          <actualResult>43</actualResult>
          <successResult>false</successResult>
          <explanation>Not cool jimmy.</explanation>
          <operator>==</operator>
          <nature>0</nature>
        </expect-field>
        <expect-field>
          <fieldName>name</fieldName>
          <expected>michael</expected>
          <actualResult>bob</actualResult>
          <successResult>true</successResult>
          <explanation>Yeah !</explanation>
          <operator>!=</operator>
          <nature>0</nature>
        </expect-field>
      </fieldValues>
      <name>d1</name>
      <anonymous>false</anonymous>
    </expect-fact>
    <expect-rule>
      <ruleName>Life unverse and everything</ruleName>
      <expectedCount>42</expectedCount>
      <explanation>All good here.</explanation>
      <successResult>true</successResult>
      <actualResult>42</actualResult>
    </expect-rule>
    <expect-rule>
      <ruleName>Everything else</ruleName>
      <explanation>Not so good here.</explanation>
      <expectedFire>true</expectedFire>
      <successResult>false</successResult>
      <actualResult>0</actualResult>
    </expect-rule>
  </fixtures>
  <rules>
    <string>rule1</string>
    <string>rule2</string>
  </rules>
  <inclusive>false</inclusive>
