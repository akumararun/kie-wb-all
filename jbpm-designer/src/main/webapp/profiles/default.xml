		<plugin name="ORYX.Plugins.UUIDRepositorySave" source="uuidRepository.js"/>
		<plugin name="ORYX.Plugins.SelectStencilSetPerspective" source="selectssperspective.js" />
		<plugin name="ORYX.Plugins.ShapeMenuPlugin" source="shapemenu.js"/>
		<plugin name="ORYX.Plugins.ShapeRepository" source="shaperepository.js"/>
		<plugin name="ORYX.Plugins.PropertyWindow" source="propertywindow.js"/>
		<plugin name="ORYX.Plugins.CanvasResize" source="canvasResize.js"/>
		<plugin name="ORYX.Plugins.View" source="view.js"/>
		<plugin name="ORYX.Plugins.DragDropResize" source="dragdropresize.js"/>
		<plugin name="ORYX.Plugins.RenameShapes" source="renameShapes.js" />
		<plugin name="ORYX.Plugins.JSONSupport" source="jsonSupport.js"/>
		<plugin name="ORYX.Plugins.Undo"  source="undo.js"/>		
		<plugin name="ORYX.Plugins.Arrangement" source="arrangement.js"/>
